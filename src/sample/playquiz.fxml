<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="1050.0" styleClass="background" stylesheets="@dashboard.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PlayQuizController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Button fx:id="startbtn" layoutX="473.0" layoutY="166.0" mnemonicParsing="false" onMouseClicked="#startquiz" styleClass="add-btn" text="Start Quiz" />
      <ScrollPane layoutX="173.0" layoutY="218.0" prefHeight="431.0" prefWidth="748.0">
         <content>
            <BorderPane fx:id="bp" prefHeight="425.0" prefWidth="726.0" styleClass="table-view">
               <bottom>
                  <Button fx:id="nextbtn" mnemonicParsing="false" styleClass="add-btn" text="Next" BorderPane.alignment="CENTER" />
               </bottom>
            </BorderPane>
         </content>
      </ScrollPane>
      <Button fx:id="resultbtn" layoutX="485.0" layoutY="166.0" mnemonicParsing="false" onMouseClicked="#result" styleClass="add-btn" text="Result" />
      <Label fx:id="correctAnswer" layoutX="176.0" layoutY="59.0" prefHeight="81.0" prefWidth="718.0" styleClass="correct" stylesheets="@dashboard.css" />
      <Label fx:id="wrongAnswer" layoutX="176.0" layoutY="59.0" prefHeight="81.0" prefWidth="718.0" styleClass="wrong" stylesheets="@dashboard.css" />
   </children>
</AnchorPane>
